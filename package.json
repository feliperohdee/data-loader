{
    "name": "rxjs-data-loader",
    "version": "1.0.5",
    "description": "This package create cached queries to avoid make duplicate requests to the backend.",
    "main": "index.js",
    "scripts": {
        "coverage": "rm -rf coverage; NODE_ENV=test nyc ./node_modules/.bin/_mocha *.spec.js --recursive --timeout 5000; rm -rf .nyc_output",
        "deploy": "npm version patch -m 'Bump %s' && git push origin master && npm publish",
        "eslint": "eslint . --ext .js --config node_modules/eslint-config/index.js",
        "test": "NODE_ENV=test ./node_modules/.bin/_mocha *.spec.js --recursive --watch --timeout 5000"
    },
    "author": "Felipe Rohde",
    "license": "ISC",
    "dependencies": {},
    "devDependencies": {
        "chai": "^4.2.0",
        "eslint": "^6.8.0",
        "graphql": "^14.6.0",
        "mocha": "^7.1.0",
        "nyc": "^15.0.0",
        "rxjs": "^6.5.4",
        "sinon": "^9.0.1",
        "sinon-chai": "^3.5.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/feliperohdee/data-loader"
    },
    "nyc": {
        "all": true,
        "reporter": "lcov",
        "exclude": [
            "*.spec.js",
            "**/*.spec.js",
            "testing/**"
        ]
    }
}
